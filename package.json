{
	"name": "idownloader",
	"version": "1.0.152",
	"author": "Jack Wang <jack.wang@integem.com>",
	"description": "youtube download tool",
	"license": "MIT",
	"homepage": "https://integem.com",
	"main": "./dist/electron/main.js",
	"scripts": {
		"prebuild": "npm run pkg && npm run build:clean",
		"build": "node --max-old-space-size=8192 .electron-vue/build.js && electron-builder",
		"upload": "node ./.electron-vue/upload.js all",
		"upload:info": "node ./.electron-vue/upload.js",
		"version": "node package.js",
		"build:dir": "node .electron-vue/build.js && electron-builder --dir",
		"build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
		"build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
		"dev": "cross-env DEV=true node .electron-vue/dev-runner.js",
		"e2e": "npm run pack && mocha test/e2e",
		"pack": "npm run pack:main && npm run pack:renderer",
		"pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
		"pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
		"test": "npm run unit && npm run e2e",
		"unit": "karma start test/unit/karma.conf.js",
		"postinstall": "cd ytdl && npm i",
		"pkg": "node ./.electron-vue/dir.js && pkg . -t node10 --out-dir ./command/",
		"command": "node ./getLink/index.js https://www.youtube.com/watch?v=DyDfgMOUjCI"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run version && git add package.json && pretty-quick --staged"
		}
	},
	"bin": {
		"mian": "getLink/index.js"
	},
	"pkg": {
		"scripts": [],
		"assets": []
	},
	"build": {
		"productName": "iDownloader",
		"appId": "com.integem.tool.download",
		"publish": [
			{
				"provider": "generic",
				"url": "https://server.integem.com/tool/"
			}
		],
		"directories": {
			"output": "build"
		},
		"files": ["package.json", "dist/electron/**/*"],
		"extraFiles": [
			"command/**/*",
			"ytdl/**/*",
			"node_modules/ffmpeg-static/**/*",
			"node_modules/sliced/**/*",
			"node_modules/debug/**/*",
			"node_modules/ms/**/*",
			"node_modules/deep-defaults/**/*",
			"node_modules/lodash/**/*"
		],
		"dmg": {
			"contents": [
				{
					"x": 410,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				},
				{
					"x": 130,
					"y": 150,
					"type": "file"
				}
			]
		},
		"mac": {
			"target": ["dmg", "pkg"],
			"icon": "build/icons/icon.icns"
		},
		"win": {
			"target": ["nsis", "MSI", "AppX"],
			"icon": "build/icons/icon.ico"
		},
		"linux": {
			"target": ["deb", "rpm", "AppImage", "apk"],
			"icon": "build/icons"
		}
	},
	"dependencies": {
		"async": "^2.6.1",
		"axios": "^0.16.1",
		"bootstrap": "^4.1.1",
		"bootstrap-vue": "^2.16.0",
		"bufferutil": "^4.0.1",
		"cheerio": "^1.0.0-rc.3",
		"element-ui": "^2.4.3",
		"ffmpeg-static": "^2.3.0",
		"jquery": "^3.3.1",
		"js-yaml": "^3.12.0",
		"moment": "^2.24.0",
		"nightmare": "^3.0.2",
		"progress-stream": "^2.0.0",
		"socket.io": "^2.1.1",
		"utf-8-validate": "^5.0.2",
		"vue": "^2.5.16",
		"vue-awesome": "^2.3.7",
		"vue-electron": "^1.0.6",
		"vue-router": "^2.5.3",
		"vuex": "^2.3.1",
		"youtube-search": "^1.1.4",
		"yt-search": "^1.1.2",
		"ytdl-core": "^1.0.9"
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-istanbul": "^4.1.6",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.24.1",
		"babili-webpack-plugin": "^0.1.2",
		"cfonts": "^1.2.0",
		"chai": "^4.0.0",
		"chalk": "^2.4.1",
		"consola": "^2.7.1",
		"copy-webpack-plugin": "^4.5.1",
		"cross-env": "^5.1.5",
		"css-loader": "^0.28.11",
		"del": "^3.0.0",
		"devtron": "^1.4.0",
		"electron": "^2.0.0",
		"electron-builder": "^21.1.6",
		"electron-debug": "^1.4.0",
		"electron-devtools-installer": "^2.2.4",
		"electron-updater": "^2.21.10",
		"extract-text-webpack-plugin": "^3.0.0",
		"file-loader": "^0.11.2",
		"html-webpack-plugin": "^2.30.1",
		"husky": "^3.1.0",
		"inject-loader": "^3.0.0",
		"karma": "^2.0.3",
		"karma-chai": "^0.1.0",
		"karma-coverage": "^1.1.2",
		"karma-electron": "^5.3.0",
		"karma-mocha": "^1.2.0",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-spec-reporter": "^0.0.31",
		"karma-webpack": "^2.0.13",
		"mocha": "^5.2.0",
		"multispinner": "^0.2.1",
		"node-loader": "^0.6.0",
		"pkg": "^4.4.0",
		"prettier": "^1.13.7",
		"pretty-quick": "^2.0.1",
		"require-dir": "^0.3.0",
		"scp": "0.0.3",
		"scp2": "^0.5.0",
		"spectron": "^3.7.1",
		"style-loader": "^0.18.2",
		"url-loader": "^1.0.1",
		"vue-html-loader": "^1.2.4",
		"vue-loader": "^13.7.2",
		"vue-style-loader": "^3.0.1",
		"vue-template-compiler": "^2.5.16",
		"webpack": "^3.12.0",
		"webpack-dev-server": "^2.11.2",
		"webpack-hot-middleware": "^2.22.2",
		"webpack-merge": "^4.1.3"
	}
}
